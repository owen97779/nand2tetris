/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    // Nota and b or a and Notb
    Not (in=a, out=nota);
    And (a=nota, b=b, out=andaout);
    Not (in=b, out=notb);
    And (a=a, b=notb, out=andbout);
    Or (a=andaout, b=andbout, out=out);

}